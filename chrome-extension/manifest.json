{
  "name": "NFSP Scanner",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Esta extensão injeta sua câmera na página de doações da Nota Fiscal Paulista.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "scripting",
    "webNavigation"
  ],
  "host_permissions": [
    "http://localhost:3000/*",
    "https://www.nfp.fazenda.sp.gov.br/*"
  ],
  "background": {
    "service_worker": "sw.js"
  },
  "action": {}
}
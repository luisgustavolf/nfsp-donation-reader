{
  "name": "NFSP Scanner",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Esta extensão injeta sua câmera na página de doações da Nota Fiscal Paulista.",
  "permissions": [
    "scripting",
    "webNavigation"
  ],
  "host_permissions": [
    "http://localhost:3000/*",
    "https://www.nfp.fazenda.sp.gov.br/*"
  ],
  "background": {
    "service_worker": "sw.js"
  },
  "action": {}
}
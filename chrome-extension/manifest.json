{
  "name": "Scripting API Demo",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Uses the chrome.scripting API to inject JavaScript into web pages.",
  "permissions": [
    "scripting",
    "webNavigation",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:3000/*",
    "https://www.nfp.fazenda.sp.gov.br/*"
  ],
  "background": {
    "service_worker": "sw.js"
  },
  "action": {}
}